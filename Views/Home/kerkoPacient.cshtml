@{
    ViewData["Title"] = "Kerko Pacient";
}

@* @model IEnumerable<BioLab.Models.Analiza> *@


 <form asp-controller="Home" asp-action="kerkoPacient">
    <p>
        Title: <input type="text" name="SearchString2" />
        <input class="btn btn-primary" type="submit" value="Filter2" />
    </p>
</form>
<div class="row">
   <div class="col sm-6">
            <table class="table table-striped table-bordered  ">
                <thead>
                    <tr>
                        <th   scope="col ">Emri</th>
                        <th   scope="col ">Password</th>
                        <th   scope="col ">Emri Analizes</th>
                        <th   scope="col ">Data</th>
                        

                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in ViewBag.Flete )
                    {
                    <tr>
                        <td scope="row"> @item.MyPacient.Emripacientit</td>
                        <td scope="row"> @item.MyPacient.Password</td>
                        <td scope="row"><a asp-action="printo2" asp-route-id2="@item.FleteAnalizeId">@item.Emri </a></td>
                        <td scope="row"> @item.CreatedAt</td>
                    </tr>
                         }
        </tbody>
    </table>

